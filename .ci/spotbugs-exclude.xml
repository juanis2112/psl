<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter
            xmlns="https://github.com/spotbugs/filter/3.0.0"
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:schemaLocation="https://github.com/spotbugs/filter/3.0.0 https://raw.githubusercontent.com/spotbugs/spotbugs/3.1.0/spotbugs/etc/findbugsfilter.xsd">

    <Match>
        <Bug pattern="EI_EXPOSE_REP" />
    </Match>

    <Match>
        <Bug pattern="EI_EXPOSE_REP2" />
    </Match>

    <!-- Incompatibility between spotbugs and Java 11: https://github.com/spotbugs/spotbugs-maven-plugin/issues/92 -->
    <Match>
        <Bug pattern="RCN_REDUNDANT_NULLCHECK_WOULD_HAVE_BEEN_A_NPE" />
    </Match>

    <Match>
        <Class name="org.linqs.psl.application.learning.weight.search.bayesian.SquaredExpKernel" />
        <Method name="kernel" />
        <Bug pattern="RV_RETURN_VALUE_IGNORED_INFERRED" />
    </Match>

    <Match>
        <Class name="org.linqs.psl.database.AtomStore" />
        <Or>
            <Field name="numAtoms" />
            <Field name="atomValues" />
            <Field name="atoms" />
            <Field name="lookup" />
        </Or>
        <Bug pattern="IS2_INCONSISTENT_SYNC" />
    </Match>

    <Match>
        <Class name="org.linqs.psl.reasoner.duallcqp.term.DualLCQPAtom" />
        <Bug pattern="UG_SYNC_SET_UNSYNC_GET" />
    </Match>

    <Match>
        <Class name="org.linqs.psl.database.Database" />
        <Field name="atomStore" />
        <Bug pattern="IS2_INCONSISTENT_SYNC" />
    </Match>

    <Match>
        <Class name="org.linqs.psl.database.rdbms.PredicateInfo" />
        <Bug pattern="SQL_NONCONSTANT_STRING_PASSED_TO_EXECUTE" />
    </Match>

    <Match>
        <Class name="~org.linqs.psl.database.rdbms.driver..*Driver" />
        <Bug pattern="SQL_PREPARED_STATEMENT_GENERATED_FROM_NONCONSTANT_STRING" />
    </Match>

    <Match>
        <Class name="org.linqs.psl.evaluation.statistics.AUCEvaluator" />
        <Method name="getLabel" />
        <Bug pattern="NP_BOOLEAN_RETURN_NULL" />
    </Match>

    <Match>
        <Class name="~org.linqs.psl.grounding.collective.SearchFringe.*" />
        <Bug pattern="BC_BAD_CAST_TO_CONCRETE_COLLECTION" />
    </Match>

    <!-- Ignore all files generated by antlr. -->
    <Match>
        <Package name="org.linqs.psl.parser.antlr" />
    </Match>

    <Match>
        <Class name="org.linqs.psl.reasoner.admm.ADMMReasoner" />
        <Or>
            <Field name="AxNorm" />
            <Field name="AyNorm" />
            <Field name="BzNorm" />
            <Field name="lagrangePenalty" />
            <Field name="augmentedLagrangePenalty" />
        </Or>
        <Bug pattern="IS2_INCONSISTENT_SYNC" />
    </Match>

    <Match>
        <Class name="org.linqs.psl.reasoner.term.DummyTermStore" />
        <Field name="count" />
        <Bug pattern="IS2_INCONSISTENT_SYNC" />
    </Match>

    <Match>
        <Class name="org.linqs.psl.reasoner.admm.term.ADMMTermStore" />
        <Field name="numLocalVariables" />
        <Bug pattern="IS2_INCONSISTENT_SYNC" />
    </Match>

    <Match>
        <Class name="org.linqs.psl.reasoner.term.MemoryVariableTermStore" />
        <Method name="updateModelAtoms" />
        <Bug pattern="WMI_WRONG_MAP_ITERATOR" />
    </Match>

    <Match>
        <Class name="org.linqs.psl.reasoner.term.MemoryConsensusTermStore" />
        <Field name="numLocalVariables" />
        <Bug pattern="IS2_INCONSISTENT_SYNC" />
    </Match>

    <Match>
        <Class name="org.linqs.psl.reasoner.term.streaming.StreamingTermStore" />
        <Or>
            <Field name="numObservedAtoms" />
            <Field name="numRandomVariableAtoms" />
        </Or>
        <Bug pattern="IS2_INCONSISTENT_SYNC" />
    </Match>

    <!-- Subclasses that are used for JSON data binding and use public fields. -->
    <Match>
        <Or>
            <Class name="~org.linqs.psl.runtime.GroundingAPI.*" />
            <Class name="~org.linqs.psl.runtime.RuntimeConfig.*" />
            <Class name="~org.linqs.psl.runtime.RuntimeResult.*" />
        </Or>
        <Or>
            <Bug pattern="URF_UNREAD_PUBLIC_OR_PROTECTED_FIELD" />
            <Bug pattern="UUF_UNUSED_PUBLIC_OR_PROTECTED_FIELD" />
        </Or>
    </Match>

    <!-- RuntimeConfig and its subclasses use equals() in a very limted manner. -->
    <Match>
        <Class name="~org.linqs.psl.runtime.RuntimeConfig.*" />
        <Or>
            <Bug pattern="HE_EQUALS_USE_HASHCODE" />
        </Or>
    </Match>

    <!-- Jackson value filtering (to remove null/empty values during serialization) uses the equals() method to filter out values. -->
    <Match>
        <Class name="org.linqs.psl.runtime.RuntimeConfig$EmptyValueFilter" />
        <Or>
            <Bug pattern="EQ_CHECK_FOR_OPERAND_NOT_COMPATIBLE_WITH_THIS" />
            <Bug pattern="EQ_UNUSUAL" />
            <Bug pattern="HE_EQUALS_USE_HASHCODE" />
        </Or>
    </Match>

    <!-- These checks come from working directly from System.out (which we cannot set an encoding for at this point). -->
    <Match>
        <Class name="org.linqs.psl.runtime.Runtime$GroundRuleOutputter" />
        <Bug pattern="DM_DEFAULT_ENCODING" />
    </Match>

    <Match>
        <Class name="org.linqs.psl.util.RandUtils" />
        <Bug pattern="DMI_RANDOM_USED_ONLY_ONCE" />
    </Match>

    <!-- Rules explicitly define their hashcode to be the rules' identity hash code (object id). Rules are generally meant to be created once. -->
    <Match>
        <Class name="~org.linqs.psl.model.rule..*Rule.*" />
        <Bug pattern="HE_EQUALS_NO_HASHCODE,HE_EQUALS_USE_HASHCODE" />
    </Match>

</FindBugsFilter>
